<h1>Edit User Profile</h1>

<%= simple_form_for @user do |f| %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :diet, as: :select, include_blank: false, priority: ["Default"], collection: ["Default", "Vegetarian", "Gluten Free"] %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.submit %>
<% end %>